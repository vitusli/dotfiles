name "Cursor Dark Midnight"

base {
    appearance "dark"
    background #191c22
    separator #272c36
    text #7b88a1
}

actionBar {
    button {
        background #191c22
        hotkey #88c0d0
    }
}

asyncProgressBar {
    border #434c5e
    progress #88c0d0
    itemProgress #d8dee9
}


breadcrumbs {
    color1 #191c22
    color2 #191c22
    color3 #191c22
    separator #272c36
}

files {
    background {
        alternate #191c22
        current #1e2127
        currentSelected #434c5e
        selected #00000000
    }

    dragDropSelection {
        width 1
    }

    text {
        current #d8dee9
        currentSelected #88c0d0
        hidden #4c566a
        matched #88c0d0
        selected #88c0d0

        highlighting1 #bf616a
        highlighting2 #d08770
        highlighting3 #ebcb8b
        highlighting4 #a3be8c
    }
}

searchBar {
    query {
        placeholder #4c566a
        text #d8dee9
    }

    results {
        highlighting #88c0d0
        selection #21242b
        subtitle #4c566a
        text #d8dee9
    }

    rowSeparator {
        color #434c5e
        show false
    }
}

statusBar {
    quickSelectNoResultsHighlighting #bf616a
}

tableHeader {
    pressed {
        background #434c5e
    }
}

tabs {
    background #191c22

    active {
        background #1e2127
        text #d8dee9
    }

    inactive {
        background #191c22
        text #4b5163
    }
}

terminal {
    caret {
        background #8fbcbb
        text #191c22
    }
}

textEditor {
    error #bf616a
    warning #ebcb8b
    keyword #81a1c1
    comment #8597bc
    identifier #88c0d0

    literal {
        string #a3be8c
        number #b48ead
        color #a3be8c
    }

    gutter {
        background #1e2127
        text #4c566a
    }
}

